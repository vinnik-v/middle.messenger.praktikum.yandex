<div class="form{{#if className}} {{className}}{{/if}}">
        <div class="form__title">
            {{ formTitle }} {{ fields.[0].fieldName }}
        </div>
        <div class="form__fields">
            {{#each (form-fields helperParam) }}
                {{> FormInputField }}
            {{/each}}
        </div>
        <div class="form__buttons">
            {{#each (form-button helperParam) }}
                {{> FormButton }}
            {{/each}}
        </div>
    </div>